{
  "manifest_version": 2,

  "name": "GitHub Project Filters",
  "description": "Adds useful filters to your GitHub project board",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": [ "https://*.github.com/*/*/projects/*"],
      "css":     ["bundle.css"],
      "js":      ["bundle.js"]
    }
  ]
}
